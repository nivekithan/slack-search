// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Message {
  id        String @id
  teamId    String
  channelId String
  userId    String
  messageTs String

  channel Channel @relation(fields: [channelId, teamId], references: [channelId, teamId])
  message String

  @@unique([channelId, userId, messageTs, teamId])
}

model Channel {
  id        String @id
  teamId    String
  channelId String

  channelName String

  Message Message[]

  @@unique([teamId, channelId])
}
